<h1>Listing calculators</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Telephone</th>
      <th>Post code</th>
      <th>Current rent</th>
      <th>Initial cost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calculators.each do |calculator| %>
      <tr>
        <td><%= calculator.email %></td>
        <td><%= calculator.telephone %></td>
        <td><%= calculator.post_code %></td>
        <td><%= calculator.current_rent %></td>
        <td><%= calculator.initial_cost %></td>
        <td><%= link_to 'Show', calculator %></td>
        <td><%= link_to 'Edit', edit_calculator_path(calculator) %></td>
        <td><%= link_to 'Destroy', calculator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calculator', new_calculator_path %>
